<h1>Products!</h1>
  <%= link_to "new product", new_product_path %>
  <br>
  <%= render 'search', q:@q, url: products_path %>
  <br>
<% if @products.present? %>
  <h3>product list</h3>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Price</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'product', collection: @products %>
    </tbody>
  </table>
  <%= paginate @products %>
<% else %>
  <p>~~None product~~</p>
<% end %>
<br>
<div>kaminari_demo</div>
